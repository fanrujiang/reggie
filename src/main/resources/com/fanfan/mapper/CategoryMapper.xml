<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fanfan.mapper.CategoryMapper">
    <select id="findPage" resultType="com.fanfan.pojo.Category">
        select *from category
        order by update_time desc
    </select>
    <select id="list" resultType="com.fanfan.pojo.Category">
        select *
        from category
        where type = #{type}
    </select>
</mapper>